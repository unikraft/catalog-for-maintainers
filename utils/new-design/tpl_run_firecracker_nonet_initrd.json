{{
  "boot-source": {{
    "kernel_image_path": "{kernel}",
    "boot_args": "{name} vfs.fstab=[ \"initrd0:/:extract::ramfs=1:\" ] -- {cmd}",
    "initrd_path": "{run_dir}/initrd.cpio"
  }},
  "drives": [],
  "machine-config": {{
    "vcpu_count": 1,
    "mem_size_mib": 8,
    "smt": false,
    "track_dirty_pages": false
  }},
  "cpu-config": null,
  "balloon": null,
  "vsock": null,
  "logger": {{
    "log_path": "/tmp/firecracker.log",
    "level": "Debug",
    "show_level": true,
    "show_log_origin": true
  }},
  "metrics": null,
  "mmds-config": null,
  "entropy": null
}}
