hello:
        Path("/hello")
        -> status(200)
        -> inlineContent("Hello, World!")
        -> <shunt>;

null:
        Path("/null")
        -> "http://172.32.255.1/null";

gold:
        Path("/gold")
        -> "http://172.32.255.1/gold";

oneb:
        Path("/1b")
        -> "http://172.32.255.1/1b";

rest: *
        -> status(404)
        -> inlineContent("No route entry")
        -> <shunt>;
